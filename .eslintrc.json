{
  "extends": [
    "airbnb"
  ],
  "rules": {
    "react/forbid-prop-types": [
      "error",
      {
        "forbid": [
          "any"
        ]
      }
    ],
    "guard-for-in": 0,
    "no-restricted-syntax": 0
  },
  "globals": {
    "localStorage": true,
    "window": true,
    "sessionStorage": true,
    "document": true,
    "FileReader": true
  }
}